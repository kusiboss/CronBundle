{
    "name": "whatwedo/cron-bundle",
    "type": "symfony-bundle",
    "description": "Run Symfony commands as cron job",
    "license": "MIT",
    "homepage": "https://doc.whatwedo.ch/whatwedo/cronbundle",
    "authors": [
        {
            "name": "Felix Imobersteg",
            "email": "felix@whatwedo.ch"
        },
        {
            "name": "Felix Ammann",
            "email": "fexu@whatwedo.ch"
        }
    ],
    "require": {
        "php": ">=8",
        "ext-posix": "*",
        "cocur/background-process": "~0.7",
        "doctrine/doctrine-bundle": "~1.9|^2.0",
        "doctrine/orm": "~2.6",
        "dragonmantank/cron-expression": "~3.3",
        "symfony/console": "~6.0|~5.0",
        "symfony/event-dispatcher": "~6.0|~5.0",
        "symfony/framework-bundle": "~6.0|~5.0",
        "symfony/process": "~6.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "whatwedo\\CronBundle\\": "src/",
            "whatwedo\\CronBundle\\Tests\\": "tests/"
        }
    }
}
